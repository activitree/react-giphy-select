{
  "name": "react-giphy-select",
  "version": "1.0.6",
  "description": "A React component for select GIFs on GIPHY.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:lib",
    "build:lib": "babel src -d lib --ignore test.js",
    "build:clean": "rimraf lib",
    "lint": "eslint ./",
    "version": "npm run build",
    "start": "webpack-dev-server",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "build-docs": "rimraf docs-dist && NODE_ENV=production webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WEACOMRU/react-giphy-select.git"
  },
  "keywords": [
    "react",
    "giphy",
    "gif"
  ],
  "author": "bashkos <bashkos@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WEACOMRU/react-giphy-select/issues"
  },
  "homepage": "https://github.com/WEACOMRU/react-giphy-select#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "postcss-loader": "^3.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.15",
    "react-inspector": "^4.0.0",
    "react-syntax-highlighter": "^11.0.2",
    "react-test-renderer": "^16.10.2",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.8",
    "react-custom-scrollbars": "^4.1.1",
    "react-masonry-component": "^6.2.1"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
