{
  "name": "@activitree/react-giphy-select",
  "version": "1.0.23",
  "description": "A React component for select GIFs on GIPHY.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --copy-files --source-maps",
    "build:lib": "babel src -d lib --ignore test.js",
    "build:clean": "rimraf lib",
    "lint": "eslint ./",
    "version": "npm run build",
    "start": "webpack-dev-server",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "build-docs": "rimraf docs-dist && NODE_ENV=production webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activitree/react-giphy-select.git"
  },
  "keywords": [
    "react",
    "giphy",
    "gif"
  ],
  "author": "bashkos <bashkos@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WEACOMRU/react-giphy-select/issues"
  },
  "homepage": "https://github.com/WEACOMRU/react-giphy-select#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "postcss-loader": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@activitree/react-masonry-component": "^6.3.6",
    "isomorphic-fetch": "^3.0.0",
    "prop-types": "^15.8.1",
    "react-custom-scrollbars-2": "^4.5.0"
  },
  "peerDependencies": {
    "react": "^17.0.1 || ^18.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
